{
  "manifest_version": 3,
  "name": "Mind Archive Extension",
  "version": "1.2",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": ["storage", "scripting"],
    "host_permissions": ["http://localhost:3000/*"]
  }
}
